app:
  datasource:
    jdbc-url: jdbc:postgresql://localhost:5432/forar
    username: {postgres}
    password: {password}
    pool-size: 30
spring:
  flyway:
    baseline-on-migrate: true
okta:
  oauth2:
    issuer: {org-url}/oauth2/default
    client-id: {client-id}
    client-secret: {client-secret}
    redirect-uri: {login-redirect-uri (e.g. /authorization-code/callback}
