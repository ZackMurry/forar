app:
  datasource:
    jdbc-url: jdbc:postgresql://localhost:5432/forar
    username: {postgres}
    password: {password}
    pool-size: 30
spring:
  flyway:
    baseline-on-migrate: true
  security:
    oauth2:
      client:
        registration:
          okta:
            client-id: {oktaClientId}
            client-secret: {clientSecret}
            scope: openid, email, profile
        provider:
          okta:
            issuer-uri: https://{yourOktaDomain}/oauth2/default
